<div class="card">
  <div class="card-body">
    <h5 class="card-title">
      <%= link_to project.title, project %>
      <% project.technologies.each do |technology| %>
        <a href="<%= technology.url %>">
          <img src="<%= technology.icon_url %>" alt="<%= technology.name %>" style="width: 24px; height: 24px;" />
        </a>
      <% end %>
    </h5>
    <div class="card-text">
      <%= project.description %>
    </div>
    <div class="card-text">
    </div>
  </div>
</div>